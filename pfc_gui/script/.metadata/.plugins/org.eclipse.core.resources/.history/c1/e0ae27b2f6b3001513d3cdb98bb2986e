import wx

class Frame(wx.Frame):
    '''
    classdocs
    '''


    def __init__(self, parent, title):
        wx.Frame.__init__(self, parent, title=title, size=(200,200))
        self.control = wx.TextCtrl(self, style=wx.TE_MULTILINE)
        self.Show(True)